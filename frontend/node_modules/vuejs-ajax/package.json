{
  "name": "vuejs-ajax",
  "version": "1.3.97",
  "description": "It is a XHR plugin that works in specific features for Vue.js 2.x and and above versions. It has many similar features with `jQuery`'s `ajax()` and `Angular`'s `http()`. In addition to these, it also has its own practical features. For example, `assets`, `component shifter`, `file upload`, `history`, `title`, and `preventing duplicate request` features.",
  "keywords": [
    "vue",
    "ajax",
    "xhr",
    "file-upload",
    "dynamic Vue components",
    "async Vue components",
    "delete",
    "get",
    "head",
    "jsonp",
    "patch",
    "post",
    "put",
    "pjax",
    "pushState"
  ],
  "main": "dist/vuejs-ajax.umd.js",
  "module": "dist/vuejs-ajax.esm.js",
  "unpkg": "dist/vuejs-ajax.min.js",
  "files": [
    "src",
    "dist"
  ],
  "directories": {
    "example": "test"
  },
  "author": "Fatih Koca <fattih@fattih.com>",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/vuejs-ajax.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/vuejs-ajax.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/vuejs-ajax.min.js",
    "lint": "jshint *.js",
    "test": "mocha test/test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fattihkoca/vuejs-ajax.git"
  },
  "private": false,
  "dependencies": {},
  "devDependencies": {
    "custom-event": "^1.0.1",
    "jsdom": "^11.12.0",
    "jsdom-global": "3.0.2",
    "mocha": "^5.2.0",
    "rollup": "^0.57.1",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-vue": "^3.0.0",
    "rollup-plugin-vue2": "^0.8.1",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11",
    "xmlhttprequest": "^1.8.0"
  },
  "bugs": {
    "url": "https://github.com/fattihkoca/vuejs-ajax/issues"
  },
  "homepage": "https://github.com/fattihkoca/vuejs-ajax#readme"
}
